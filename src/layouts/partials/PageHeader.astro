---
import { humanize } from "@/lib/utils/textConverter";

const { title = "" }: { title?: string } = Astro.props;
---

<section>
  <div class="container text-center">
    <div class="px-8 py-10 font-normal">
      <h1 set:text={humanize(title)} class="font-normal" />
    </div>
  </div>
</section>
